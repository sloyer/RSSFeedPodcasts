{
  "functions": {
    "api/cron.js": {
      "maxDuration": 120
    },
    "api/podcasts.js": {
      "maxDuration": 10
    },
    "api/podcasts/shows.js": {
      "maxDuration": 10
    },
    "api/articles.js": {
      "maxDuration": 10
    },
    "api/feeds.js": {
      "maxDuration": 10
    },
    "api/feed-sources.js": {
      "maxDuration": 10
    },
    "api/historicalSync.js": {
      "maxDuration": 300
    },
    "api/add-youtube-channel.js": {
      "maxDuration": 30
    },
    "api/migrate-youtube.js": {
      "maxDuration": 120
    },
    "api/tweets.js": {
      "maxDuration": 10
    },
    "api/twitter/accounts.js": {
      "maxDuration": 10
    },
    "api/trigger-twitter-pull.js": {
      "maxDuration": 180
    },
    "api/cron-twitter.js": {
      "maxDuration": 180
    },
    "api/race-schedule.js": {
      "maxDuration": 10
    },
    "api/cron-race-notifications.js": {
      "maxDuration": 60
    },
    "api/cron-mute-reminder.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron-inactive-reminder",
      "schedule": "0 10,18 * * *"
    },
    {
      "path": "/api/cron-race-notifications",
      "schedule": "*/15 * * * 6"
    },
    {
      "path": "/api/cron-mute-reminder",
      "schedule": "0 12 * * 5,6"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}

